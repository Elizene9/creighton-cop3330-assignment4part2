<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.DeleteListController">
   <children>
      <Button layoutX="465.0" layoutY="324.0" mnemonicParsing="false" onAction="#ExitPressed" prefHeight="62.0" prefWidth="126.0" text="EXIT">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="324.0" mnemonicParsing="false" onAction="#MenuButtonPressed" prefHeight="62.0" prefWidth="126.0" text="MENU">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <TextArea editable="false" layoutY="152.0" prefHeight="84.0" prefWidth="605.0" text=" Are you sure you want to clear the entire list?">
         <font>
            <Font size="28.0" />
         </font>
      </TextArea>
      <Button layoutX="209.0" layoutY="237.0" mnemonicParsing="false" onAction="#ClearList" prefHeight="62.0" prefWidth="187.0" text="DELETE">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Button>
      <Button layoutX="240.0" layoutY="324.0" mnemonicParsing="false" onAction="#HelpPressed" prefHeight="62.0" prefWidth="126.0" text="HELP">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
